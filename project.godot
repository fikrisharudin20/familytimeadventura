[application]

config/name="Family Fun Adventures"
run/main_scene="res://MainMenu.tscn"
config/icon="res://icon.png"

[autoload]

Game="*res://Global/Game.gd"
Utils="*res://Global/Utils.gd"
StageManager="*res://Global/Stage_Manager.tscn"
Dragable="*res://Global/dragable.gd"
WinCond="*res://Global/WinCond.gd"

